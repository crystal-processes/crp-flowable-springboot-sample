      <design:modificationdate><![CDATA[2024-01-14T19:14:17.483Z]]></design:modificationdate>
    </extensionElements>
    <serviceTask id="CreateInsuranceEvent" name="Create Insurance Event" flowable:resultVariable="insuranceEventId" flowable:expression="${insuranceEventService.createInstance(contractId, eventDescription, requestedAmount)}">
