        <design:stencilid><![CDATA[FormTask]]></design:stencilid>
        <design:stencilsuperid><![CDATA[Task]]></design:stencilsuperid>
      </extensionElements>
    </userTask>
    <serviceTask id="sendMoneyServiceTask" name="Send money" flowable:expression="${insuranceEventService.sendMoney(insuranceEventId, amountAssessed)}">
